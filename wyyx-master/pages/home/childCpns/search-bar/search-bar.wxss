/* pages/home/childCpns/search-bar/search-bar.wxss */
.search-bar {
  width: 100vw;
  height: 68rpx;
  margin-top: 16rpx;
  padding: 0 22rpx;
  /* background-color:; */
}
.search-bar-container {
  width: 706rpx;
  height: 68rpx;
  display: flex;
  position: relative;
  border-radius: 40rpx;
  border: 2rpx #d17583;
  border-style: solid none solid  solid;
}
.search-section {
  width: 596rpx;
  height: 100%;
  padding-left: 68rpx;
}
.search-section::before {
  content: "";
  position: absolute;
  margin: 16rpx 0 0 20rpx ;
  top: 0;
  left: 0;
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,);
  background-size: 100%;
}
.search-section-title {
  width: 100%;
  height: 100%;
}
.search-section-title swiper-item {
  box-sizing: border-box;
  padding-top: 14rpx;
}
.search-bar-button {
  width: 110rpx;
  height: 100%;
  box-sizing: border-box;
  padding: 12rpx 0 0 20rpx;
  border-radius: 40rpx;
  background-color: #ee423e;
}